<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<BorderPane maxHeight="-Infinity"
            maxWidth="-Infinity"
            minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="650.0"
            prefWidth="1000.0"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml" fx:controller="App.Controller.mainPaneController">
   <center>
      <Pane fx:id="playBoard" prefHeight="600.0" prefWidth="900.0" BorderPane.alignment="CENTER" />
   </center>
   <left>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="playButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Play">
               <VBox.margin>
                  <Insets bottom="5.0" top="2.0" />
               </VBox.margin></Button>
            <ToggleButton fx:id="pauseButton_v2" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Pause">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin></ToggleButton>
            <Button fx:id="restartButton" mnemonicParsing="false" text="Restart" />
         </children>
      </VBox>
   </left>
</BorderPane>
